	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	
<mapper namespace="ldg.mybatis">


		<select id="songList"	resultType="Song"
		parameterType="hashmap">
				select * from songboard
			<if test="snum!=null">
				WHERE snum = #{snum}
			</if>
		</select>
		
